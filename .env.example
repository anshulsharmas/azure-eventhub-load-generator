# Example environment configuration
# Copy this file to .env and fill in your values

# Azure Event Hub connection string
# Get this from Azure Portal -> Event Hubs Namespace -> Shared access policies
EVENT_HUB_CONNECTION_STRING=Endpoint=sb://your-namespace.servicebus.windows.net/;SharedAccessKeyName=your-key-name;SharedAccessKey=your-access-key

# Event Hub name
EVENT_HUB_NAME=your-event-hub-name
